<%= render :partial => 'spree/admin/shared/sub_menu/product' %>
<%= render :partial => 'spree/admin/shared/product_tabs', :locals => {:current => "Images"} %>
<%= render "spree/shared/error_messages", :target => @image %>
<div class="row">
  <div class="col-md-12">
    <%= form_for(:video, :url => admin_product_video_url(id: @product.id), :html => { :method => :put, :multipart => true }) do |f| %>
      <%= render :partial => "form", :locals => { :f => f } %>
      <%= render partial: 'spree/admin/shared/edit_resource_links', locals: {collection_url: admin_product_videos_url(id: @product.id)} %>
    <% end %>
  </div>
</div>
